{
	"name": "cedar-os-cli",
	"version": "0.1.22",
	"description": "CLI tool for scaffolding Cedar-OS projects and installing components",
	"main": "dist/index.mjs",
	"bin": {
		"cedar": "dist/index.mjs"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run clean && npm run build",
		"p": "pnpm install && npm version patch && npm publish",
		"test-link": "npm run build && npm link",
		"test-dev": "npm run dev & npm link"
	},
	"keywords": [
		"cedar-os",
		"cli",
		"components",
		"ui",
		"nextjs",
		"react",
		"mastra",
		"scaffold",
		"template"
	],
	"author": "Cedar Team",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/CedarCopilot/cedar-OS.git",
		"directory": "packages/cli"
	},
	"homepage": "https://docs.cedarcopilot.com",
	"bugs": {
		"url": "https://github.com/CedarCopilot/cedar-OS/issues"
	},
	"dependencies": {
		"@clack/prompts": "^0.7.0",
		"chalk": "^5.3.0",
		"commander": "^11.1.0",
		"cross-spawn": "^7.0.6",
		"execa": "^2.0.2",
		"fs-extra": "^11.2.0",
		"node-fetch": "^3.3.2",
		"ora": "^7.0.1",
		"picocolors": "^1.0.0"
	},
	"devDependencies": {
		"@types/cross-spawn": "^6.0.6",
		"@types/execa": "^2.0.2",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.10.6",
		"concurrently": "^8.2.0",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3"
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=18.0.0"
	}
}
