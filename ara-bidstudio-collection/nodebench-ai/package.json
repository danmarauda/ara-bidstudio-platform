{
  "name": "flex-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite --open",
    "dev:backend": "convex dev",
    "build": "vite build",
    "lint": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false && convex dev --once && vite build",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint . --fix",
    "lint:typescript": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit:ci": "vitest run --coverage --reporter=verbose --reporter=junit --outputFile=reports/junit.xml --include='**/__tests__/**/*.test.{ts,tsx}' --exclude='**/e2e*.test.{ts,tsx}' --exclude='**/*.e2e.test.{ts,tsx}'",
    "test:integration:ci": "vitest run --coverage --reporter=verbose --reporter=junit --outputFile=reports/junit-integration.xml --include='**/*.test.{ts,tsx}' --exclude='**/__tests__/**/*.test.{ts,tsx}' --exclude='**/e2e*.test.{ts,tsx}' --exclude='**/*.e2e.test.{ts,tsx}'",
    "test:convex:ci": "vitest run --reporter=verbose --reporter=junit --outputFile=reports/junit-convex.xml --include='convex/**/*.test.ts'",
    "test:e2e:ci": "vitest run --reporter=verbose --reporter=junit --outputFile=reports/junit-e2e.xml --include='**/e2e*.test.{ts,tsx}' --include='**/*.e2e.test.{ts,tsx}'",
    "test:all:ci": "vitest run --coverage --reporter=verbose --reporter=junit --outputFile=reports/junit-all.xml",
    "coverage:merge": "echo 'Coverage reports merged'",
    "coverage:report": "vitest run --coverage --reporter=text --reporter=json --reporter=html --reporter=lcov",
    "coverage:check": "node scripts/check-coverage.js",
    "report:test-summary": "node scripts/generate-test-summary.js",
    "eval": "tsx scripts/runEvaluation.ts",
    "eval:quick": "convex run tools/evaluation/quickTest:runQuickTest",
    "eval:all": "convex run tools/evaluation/comprehensiveTest:runComprehensiveTest",
    "eval:stats": "convex run tools/evaluation/comprehensiveTest:getTestStats",
    "eval:categories": "convex run tools/evaluation/comprehensiveTest:listCategories",
    "eval:docs": "convex run tools/evaluation/quickTest:testDocumentTools",
    "eval:web": "convex run tools/evaluation/quickTest:testWebSearch",
    "eval:workflow": "convex run tools/evaluation/quickTest:testWorkflow",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
